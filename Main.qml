	Timer {
		// Comprueba si es necesario recargar el modelo de datos
		id: actualizarQML
		interval: 50
		running: false
		repeat: true
		onTriggered: recargarModelo()
	}
	

	// Fuente de iconos (fontawesome)
	FontLoader {
		id: fontMaterialDesign
		// source: "../fontawesome-webfont.ttf"
		source: "fontawesome-webfont.ttf"
	}
	
	Component{
		id: page1
		// Clase CollapsibleList
		CollapsibleList{
			id: list
			anchors.fill:parent
			property alias model: jsonModel1.model
			// Clase JSONListModel
			JSONListModel {
				id: jsonModel1
				json: menu.json
				query: "$.menu[*]"
			}
			theModel: jsonModel1.model;
			onItemClicked: {
			    menu.menuSelect(opcion, tipo, title, icoChar, 0, list);
				// Recargar el modelo de datos con el contenido de la variable
				if (tipo == "C") menu.json = theRoot.varToString("MEN_GEN");//jsonModel1.json = theRoot.varToString("MEN_GEN");
			}
		}
	}
	
	StackView {
		id: stack
		//initialItem: page1
		anchors.fill: parent;
		property int currentView: 0
		property variant items:[page1.createObject()]// Para añadir diferentes menús (Cargados) a la vez
		pushEnter: Transition {
			PropertyAnimation {
				property: "opacity"
				from: 0
				to:1
			}
		}
		pushExit: Transition {
			PropertyAnimation {
				property: "opacity"
				from: 1
				to:0
			}
		}
		Component.onCompleted: {
			stack.push(stack.items[0], {}, {"destroyOnPop": true})
		}
   }
	// Para depurar
	Text {
        id: txtParrafo
        anchors.top: parent.top
        anchors.left: parent.left

        height: parent.height
        width: parent.width

        text: ""
        wrapMode: Text.WordWrap
    }

	Component.onCompleted: {
		actualizarQML.start();
		// Para depurar
        if (txtParrafo.paintedWidth > 200) {
            width = 200
        } else {
            width = txtParrafo.paintedWidth
        }
    }  	
   
	function menuSelect(opcion, tipo, title, icoChar, pag, li) {	 
		 switch(tipo) {
			case "A": // Ejecutar acción
				theMainWindow.runAction(opcion);
				break;
			case "P": // Ejecutar proceso
				theRoot.runProcess(opcion);
				break;
			case "C": // Cambio de menú
				theApp.setGlobalVar("Iberdrola_Dat/CUR_MEN", opcion);
				theRoot.dataView().parentDataView().control("QML").hide()
				theRoot.dataView().parentDataView().control("QML").show()
				break;
			default:
				break;
		}
	}
	
